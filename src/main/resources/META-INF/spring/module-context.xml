<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:batch="http://www.springframework.org/schema/batch"
	xsi:schemaLocation="
	http://www.springframework.org/schema/batch http://www.springframework.org/schema/batch/spring-batch-2.1.xsd
	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<description>NFE job to get you started. It provides a skeleton for a typical batch application.</description>
 	
 	<bean id="applicationRecieveGoodRightTask" class="co.th.ktc.nfe.batch.main.ApplicationRecieveGoodRightTask" />
 	
	<batch:job id="J000">
		<batch:step id="S3001">
			<batch:tasklet ref="applicationRecieveGoodRightTask" />
		</batch:step>
	</batch:job> 
	
</beans>
